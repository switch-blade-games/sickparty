            <string>// create post processing surfaces
if (!surface_exists(surf_temp_a))
    surf_temp_a = surface_create(400,240);
if (!surface_exists(surf_temp_b))
    surf_temp_b = surface_create(400,240);
var last = application_surface;
var next = surf_temp_a;

if (global.pause)
    {
    // prepare pause surface for post
    if (!surface_exists(global.surf_pause))
        {
        global.surf_pause = surface_create(400,240);
        if (file_exists(global.CFGsurf))
            {
            // load pause surface from buffer
            var temp_buff = buffer_load(global.CFGsurf);
            buffer_set_surface(temp_buff,global.surf_pause,0,0,0);
            buffer_delete(temp_buff);
            }
        }
    var last = global.surf_pause;
    }
else
    {
    if (surface_exists(global.surf_pause))
        {
        surface_free(global.surf_pause);
        global.surf_pause = -1;
        }
    }

// draw application surface to surf_temp_a
surface_set_target(next);
draw_clear(background_color);
draw_surface(last,0,0);
surface_reset_target();
last = surf_temp_a;
next = surf_temp_b;

// apply wavy shader
if (wavy &gt; 0)
    {
    surface_set_target(next);
    draw_clear(background_color);
    shader_set(shd_wavy);
    shader_set_uniform_f(u_time,get_timer()/1000000);
    shader_set_uniform_f(u_wavy,wavy);
    shader_set_uniform_f(u_xFreq,x_freq);
    shader_set_uniform_f(u_xAmp,x_amp);
    shader_set_uniform_f(u_yFreq,y_freq);
    shader_set_uniform_f(u_yAmp,y_amp);
    draw_surface(last,0,0);
    shader_reset();
    surface_reset_target();
    last = surf_temp_b;
    next = surf_temp_a;
    }

