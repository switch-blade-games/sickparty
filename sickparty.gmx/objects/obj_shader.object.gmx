// create final surface
if (!surface_exists(surf_final))
    surf_final = surface_create(400,240);

// apply crt shader
surface_set_target(surf_final);
draw_clear($000000);
shader_set(shd_crt);
shader_set_uniform_f(u_crt,global.CFG[config.CRTeffect]);
draw_surface(last,0,0);
shader_reset();
surface_reset_target();

// final render
draw_surface(surf_final,0,0);

// screen flash
if (flash &gt; 0)
    {
    flash--;
    draw_set_color($FFFFFF);
    draw_rectangle(0,0,view_wview[0],view_hview[0],0);
    draw_set_color($000000);
    }
