  <code>global.DEBUG = false;
global.OUTPUT = true;
global.PLAYERS=1;
global.P1LIVES=4;
global.P2LIVES=4;
global.CHECKPOINT=1;

randomize();

// joystix font
global.fnt_joystix = font_add_sprite(joystix_font_spr,ord('!'),true,1);
draw_set_font(global.fnt_joystix);

// game enums
enum tile_side
    {
    none = 0,
    right = 1,
    top = 2,
    left = 4,
    bottom = 8,
    full = 15
    }

// game variables
global.moto_floor = 210;

if (!instance_exists(dev_cheater))
    instance_create(0,0,dev_cheater);

// player skin enums
enum skin
    {
    stone,
    pumpkin,
    foxford,
    yolo,
    size
    }

// player skin
global.skin_tag[skin.stone]     = "101-stone";
global.skin_tag[skin.pumpkin]   = "102-pumpkin";
global.skin_tag[skin.foxford]   = "103-fox";
global.skin_tag[skin.yolo]      = "104-yolo";

for(var i=0; i&lt;skin.size; i++;)
    {
    global.player_sprite[i] = -1;
    global.frame_info[i] = -1;
    global.anim_info[i] = -1;
    global.frames[i] = 0;
    global.autosprite_rate = room_speed;
    global.autosprite_time = room_speed;
    global.sheet_sha1[i] = "";
    global.json_sha1[i] = "";
    }

// unpack skins
var success = -1;
if (!directory_exists(working_directory+"skins\"))
    directory_create(working_directory+"skins\");
if (file_exists(working_directory+"skins.zip"))
    success = zip_unzip(working_directory+"skins.zip",working_directory+"skins\");
if (success &lt;= 0)
    log("[ERROR] MISSING SKIN FILES!");
else
    log("Successfully unpacked ["+string(success)+"] skin files!");

// load skins
for(var i=0; i&lt;skin.size; i++;)
    anim_load_skin(i);

room_goto_next();</code>
